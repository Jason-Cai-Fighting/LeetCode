//how to return a vector
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i = 0,j = 0;
        int n = nums.size();
        for(i = 0; i < n; i++){
            for(j=i+1; j < n; j++){
                if(target == nums[i] + nums[j]){
                    return {i,j};
                }                    
            }
        }
        return {};

    }
};
